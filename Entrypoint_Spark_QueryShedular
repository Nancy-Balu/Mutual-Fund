delete from dbo.wms.spark_users_EntrypointMF where dt=current_Date-1;
insert into dbo.wms.spark_users_EntrypointMF as 
 Select *, 'Spark_Android' as platform from clickstream.db_dbo_clickstream_data_tablename_dp_spark_clickstream_data where dt= current_date-1 and event_id in ('11.0.0.1.66', '11.0.0.1.85','5.0.0.40.1')  
union all
select *,'Spark_iOS' as platform from clickstream.db_dbo_clickstream_data_tablename_spark_ios_clickstream_data where dt=current_date-1 and  event_id in ('11.0.0.1.66', '11.0.0.1.85','5.0.0.40.1')
