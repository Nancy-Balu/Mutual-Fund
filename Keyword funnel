create table nancy.keyword_funnel as
with clickstream as 
(
select *,(timestamp 'epoch' + client_timestamp_unix/1000 * INTERVAL '1 second') AT TIME ZONE 'UTC' AT TIME ZONE 'Asia/Kolkata'  as cli_timestamp_ist from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data where event_id in('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0','300.0.0.1.3','300.0.0.1.2') and dt>='2023-07-01'
),
raw_data1 as 
(
select distinct client_id as clients,dt,  'Step1' as steps, 'Search_click' as step_name, cli_timestamp_ist,dt from clickstream where event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') 
union all
select  distinct client_id as clients,dt, 'Step2' as steps, 'Keyword_Search'as step_name,cli_timestamp_ist, dt from  clickstream where   event_id in ('300.0.0.1.3') and client_id in (select  distinct client_id from clickstream  where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0'))
union all
select distinct client_id as clients, dt, 'Step3' as steps, 'FundDetails_Clicked' as step_name,cli_timestamp_ist,dt  from clickstream where event_id in ('300.0.0.1.2') and client_id in(select distinct client_id as clients from clickstream where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') and client_id in(select  distinct client_id from clickstream  where event_id in ('300.0.0.1.3') ))
union all
select distinct client_id as clients, dt, 'Step3' as steps, 'FundDetails_NotClicked' as step_name,cli_timestamp_ist,dt  from clickstream where event_id in ('300.0.0.1.3') and client_id not in (select  distinct client_id from clickstream  where event_id in ('300.0.0.1.2') and
client_id in(select distinct client_id as clients from clickstream where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') and client_id in(select  distinct client_id from clickstream  where event_id in ('300.0.0.1.3') ))
))

select count(distinct clients) as clients,step_name
 ,cli_timestamp_ist::date as dtt from raw_Data1 where cli_timestamp_ist::date>='2023-07-01' and cli_timestamp_ist::date<current_date
group by 2,3


-------------Search Granular------------
create table nancy.keywordsearched as
with keywords as
(
    select * from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data where event_id='300.0.0.1.3' and dt>='2023-07-01'
),
Top_10_Keyword as
(
select trim(UPPER(substring(json_extract_path_text(lower(event_metadata), 'fundnamesearched'),1,4))) as substring,
count(substring) as Keyword_count,
date_trunc('month',dt::date) as Month,
row_number() over(partition by Month order by Keyword_count desc ) as rn
from keywords
where event_id='300.0.0.1.3' 
group by 1,3
order by 2 desc
)

select
UPPER(SPLIT_PART(replace(replace(SPLIT_PART(event_metadata, ':', 2),'"',''),'}',''),',',1)) as KeyWordSearched,
count(keywordSearched) as Keyword_count,
trim(UPPER(substring(KeyWordSearched,1,4))) as Substring,
date_trunc('month',dt::date) as Month
from keywords
where event_id='300.0.0.1.3'  and (substring,Month) IN( select substring,Month from Top_10_Keyword where rn<=10)
group by 4,3,1
order by 2 desc

----------avgamount
create table nancy.keywordsearchedavgamount as 

with raw_data1 as 
(
select distinct client_id as clients,dt,  'Step1' as steps, 'Search_click' as step_name,(timestamp 'epoch' + client_timestamp_unix/1000 * INTERVAL '1 second') AT TIME ZONE 'UTC' AT TIME ZONE 'Asia/Kolkata'  as cli_timestamp from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data  where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') and dt >='2023-07-01'
union all
select  distinct client_id as clients,dt, 'Step2' as steps, 'Fund_details'as step_name, (timestamp 'epoch' + client_timestamp_unix/1000 * INTERVAL '1 second') AT TIME ZONE 'UTC' AT TIME ZONE 'Asia/Kolkata'  as cli_timestamp from  clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data where   event_id in ('300.0.0.1.2') and client_id in (select  distinct client_id from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data  where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') and dt >='2023-07-01') and dt >='2023-07-01'
union all
select distinct client_id as clients, dt, 'Step3' as steps, 'Search_invest' as step_name,(timestamp 'epoch' + client_timestamp_unix/1000 * INTERVAL '1 second') AT TIME ZONE 'UTC' AT TIME ZONE 'Asia/Kolkata'  as cli_timestamp  from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data where event_id in ('301.0.0.1.12','301.0.0.1.14') and client_id in(select distinct client_id as clients from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') and client_id in(select  distinct client_id from clickstream.db_dbo_clickstream_data_tablename_mf_clickstream_data  where event_id in ('300.0.0.1.2') and dt >='2023-07-01') and dt >='2023-07-01') and client_id is not null and dt >='2023-07-01'),

client_invested_SIP as

(SELECT distinct client_code as client_id,id as orders,dt as Sip_date, cast(updated_ts as date) updated_date, date_trunc('month',CAST(created_ts as date)) as Mt,cast(created_ts as date) as dt,platform, status, payment_status, sip_type, installment_amount,mandate_id,payment_mode,first_order FROM dbo_wms_s3.sips where(exchange_sip_id <> '' and exchange_sip_id is not null and status not in ('SIP_REJECTED','SIP_FAILED')) and created_ts::timestamp::date>='2023-07-01'),
clients_invested_final as 
(
select distinct b.client_id,b.dt,b.installment_amount,b.orders,b.mt from raw_data1 a
inner join client_invested_SIP b on a.clients=b.client_id
and a.dt=b.dt
where a.steps='Step3'),


raw_data3 as 
(
select sum(installment_amount)/count(orders) as avg ,count(orders) as Sip_ID,count(distinct client_id) as unique_user,'Total_Sip' as category,mt from
client_invested_SIP
group by 4,5
union all
select sum(installment_amount)/count(orders) as avg ,count(orders) as Sip_ID,count(distinct client_id) as unique_user,'Search_Sip' as category,mt from
clients_invested_final
group by 4,5
)

select * from raw_data3
