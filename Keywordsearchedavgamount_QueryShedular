delete from
    dbo_wms.keywordsearchedavgamount
where
    dt >= current_date -2 and dt<current_date;
insert into
   dbo_wms.keywordsearchedavgamount

with clickstream as (
    select * from "dbo-angels"."clickstream_internal"."mf_clickstream_data" where event_id IN('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0','300.0.0.1.2','301.0.0.1.14')
and  dt >= current_date -2 and dt<current_date
),
raw_data1 as 
(
select distinct client_id as clients,dt,  'Step1' as steps, 'Search_click' as step_name,date_trunc('month',api_timestamp::date) as Mt from clickstream  where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0')
union all
select  distinct client_id as clients,dt, 'Step2' as steps, 'Fund_details'as step_name, date_trunc('month',api_timestamp::date) as Mt from  clickstream where   event_id in ('300.0.0.1.2') and client_id in (select  distinct client_id from clickstream  where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') )
union all
select distinct client_id as clients, dt, 'Step3' as steps, 'Search_invest' as step_name,date_trunc('month',api_timestamp::date) as Mt  from clickstream where event_id in ('300.0.0.1.2','301.0.0.1.14') and client_id in(select distinct client_id as clients from clickstream where   event_id in ('300.0.0.1.3','300.0.0.1.2','300.0.0.1.1','300.0.0.1.0') and client_id in(select  distinct client_id from clickstream  where event_id in ('300.0.0.1.2') and dt >= current_date -2 and dt<current_date) and client_id is not null)),

client_invested_SIP as

(SELECT distinct client_code as client_id,id as orders,dt as Sip_date, cast(updated_ts as date) updated_date, date_trunc('month',CAST(created_ts as date)) as Mt,cast(created_ts as date) as dt,platform, status, payment_status, sip_type, installment_amount,mandate_id,payment_mode,first_order FROM dbo_wms_s3.sips where(exchange_sip_id <> '' and exchange_sip_id is not null and status not in ('SIP_REJECTED','SIP_FAILED')) and created_ts::timestamp::date>=current_date-2 and created_ts::timestamp::date<current_Date),
clients_invested_final as 
(
select distinct b.client_id,b.dt,b.installment_amount,b.orders,b.mt from raw_data1 a
inner join client_invested_SIP b on a.clients=b.client_id
and a.dt=b.dt
where a.steps='Step3'),


raw_data3 as 
(
select sum(installment_amount)/count(orders) as avg ,count(orders) as Sip_ID,count(distinct client_id) as unique_user,'Total_Sip' as category,mt from
client_invested_SIP
group by 4,5
union all
select sum(installment_amount)/count(orders) as avg ,count(orders) as Sip_ID,count(distinct client_id) as unique_user,'Search_Sip' as category,mt from
clients_invested_final
group by 4,5
)

select * from raw_data3
